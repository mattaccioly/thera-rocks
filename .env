# Paste your key after the equals sign and save this file
GEMINI_API_KEY=

# Optional: customize model
GEMINI_MODEL=gemini-1.5-flash

# Set to false to use the real LLM once GEMINI_API_KEY is set
OFFLINE_MODE=false

# Database (SQLite by default)
DB_URL=sqlite:///thera_data.db

# Networking & crawling
HTTP_TIMEOUT_SECONDS=20
USER_AGENT=TheraRocksBot/1.0 (+https://thera.rocks)
MAX_PAGES_PER_DOMAIN=5
CRAWL_MAX_DEPTH=2
